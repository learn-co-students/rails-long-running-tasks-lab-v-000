<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.submit %>
<% end %>

<h2> Upload a CSV</h2>
<%= form_tag songs_upload_path, multipart: true do%>
  <%=file_field_tag :file%>
  <%= submit_tag "Upload Songs"%>
<%end%>
