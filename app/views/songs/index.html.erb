<h1>Upload Songs and Artists</h1>
<%= form_tag songs_upload_path, multipart:true do %>

  <%= file_field_tag :file %><br>
  <%= submit_tag "Import Leads" %>
<% end %>
<ul>
<% @songs.each do |song| %>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
