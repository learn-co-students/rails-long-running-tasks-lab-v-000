<%# should there be a reference to a model here? %>
<%= form_tag ({:controller => "songs", :action => "upload"}), multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import Songs" %>
<% end %>
<ul>
<% @songs.each do |song| %>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
