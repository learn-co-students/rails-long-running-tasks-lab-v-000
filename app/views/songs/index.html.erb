
<%= form_tag songs_upload_path,  multipart: true do  %>
  <%= file_field_tag :file %>
  <%= submit_tag "Upload file" %>
<% end %>

<ul>
<% @songs.each do |song| %>
  <% if !song.artist_name.blank? %>
    <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
  <% else %>
    <li><%= link_to "Add Artist", edit_song_path(song) %> - <%= link_to song.title, song_path(song) %></li>
  <% end %>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
