<ol>
<% @songs.each do |song| %>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %><br>
    </li>
<% end %>
</ol>
<br>
<%= link_to "Add New Song", new_song_path %>

<%= form_tag songs_upload_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag %>
  <% end %>
