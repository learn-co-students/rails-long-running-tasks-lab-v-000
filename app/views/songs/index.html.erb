<!-- controls to upload a file to the index view -->
<%= form_tag songs_upload_path, multipart: true do %>
<%= file_field_tag :artist_songs %>
<%= submit_tag "Import Artist Songs" %>
<% end %>
<ul>
<% @songs.each do |song| %>
<%binding.pry%>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
