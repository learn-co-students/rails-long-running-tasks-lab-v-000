<ul>
<%= form_tag upload_path, multipart: true do %>
  <%= file_field_tag :file %>
<%= submit_tag "Import Songs" %>
<% end %>
<% @songs.each do |song| %>
  <li><%= display_artist(song) %>
 <% if song.artist.nil? %>
     - <%= link_to "Add Artist", edit_song_path(song)%>
 <% else %>
   - <%= link_to song.title, song_path(song) %>
<% end %>
  </li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
